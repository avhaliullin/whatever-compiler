var x: int;
x = 9 - 2;

echo(1 + x * 30/10);

echo(x);

fn inc(a: int): int = {
    a + 1;
}

fn sum(a: int, b: int): int = {
    a + b;
}

fn sum(a: int, b: int, c: int): int = {
    a + b + c;
}

echo(inc(x));

echo(sum(x, inc(x)));

echo(sum(x, inc(x), inc(inc(x))));

echo(x < inc(x));
echo(x <= inc(x));
echo(x > inc(x));
echo(x >= inc(x));
echo(x == inc(x));
echo(x != inc(x));

fn lte(a: int, b: int): boolean = {
    a <= b;
}

echo(lte(1, 2));