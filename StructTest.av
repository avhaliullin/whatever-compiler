struct A (foo: Boolean, bar: Int)

fn echo2(st: A): Unit = {
    echo(st.foo)
    echo(st.bar)
}

fn createSt(x: Int): A = {
    var b: Boolean
    b = x == 10
    new A(foo := b, bar := x)
}

var st = createSt(10)

echo(st)

st.bar = 13

echo(st)
echo2(st)

echo(new A(bar := 14, foo := false))

echo(new A(true, bar := {
    var x = 3
    x = x + 1
}))